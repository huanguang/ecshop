<script type="text/javascript">
function tijiaos(){
    var consignee_2 = document.getElementById('consignee_2').value;
    var zipcode_2 = document.getElementById('zipcode_2').value;

    var address = document.getElementById('address_2').value;
    var tel_2 = document.getElementById('tel_2').value;
    var mobile_2 = document.getElementById('mobile_2').value;
    var sign_building_2 = document.getElementById('sign_building_2').value;
    var best_time_2 = document.getElementById('best_time_2').value;
    var selCountries_0 = document.getElementById('selCountries_').value;
    var selProvinces_0 = document.getElementById('selProvinces_').value;
    var selCities_0 = document.getElementById('selCities_').value;
    var selDistricts_0 = document.getElementById('selDistricts_').value;
    var msg = '';
    var reg = /^0?1[3|4|5|8][0-9]\d{8}$/;
    var reg2 = /^((0\d{2,3}))(\d{7,8})(-(\d{3,}))?$/;
    var pattern = /^([\.a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+(\.[a-zA-Z0-9_-])+/;
    var re= /^[1-9][0-9]{5}$/;
    if(selCountries_0 == 0){
        msg += '请选择国家\n';
    }
    if(selProvinces_0 == 0){
        msg += '请选择省份或者城市\n';
    }
    if(selCities_0 == 0){
        msg += '请选择城市\n';
    }
    if(selDistricts_0 == 0){
        msg += '请选择县区\n';
    }
    if(consignee_2.length <1){
      msg += '请输入联系人\n';
    }
    if(zipcode_2.length >0){

        if(!re.test(zipcode_2)){
          msg += '请输入正确的邮政编码\n';
        }
    }
    if(address.length <1){
      msg += '请输入详细地址\n';
    }
    if(tel_2.length >1){
      if(!reg2.test(tel_2)){
          msg += '请输入正确的电话号码\n'
      }
    }
    if(mobile_2.length <1){
      msg += '请输入手机号码\n';
    }else{
      if(!reg.test(mobile_2)){
        msg += '请输入正确的手机号码\n';
      }
    }
    if(msg.length > 0){
      alert(msg);
      return false;
    }


    document.ECS_FORMBUYs.submit();
}

